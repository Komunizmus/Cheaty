import time

n = int(input("Napis cislo: "))
sum1 = 0
start_time = time.time()
for i in range(1, n):
    if n % i == 0:
        sum1 = sum1 + i
if sum == n:
    print("Cislo ", n, " je dokonale")
else:
    print("Cislo ", n, " nie je dokonale")
print("--- %s seconds ---" % (time.time() - start_time))
